{% extends 'base.html.twig' %}

{% block title %}Hello EmplacementController!{% endblock %}

{% block body %}
    {{ form_start(emplacementForm) }}

    {{ form_label(emplacementForm.type) }}
    {{ form_widget(emplacementForm.type) }}
    {{ form_errors(emplacementForm.type) }}<hr>

    {{ form_label(emplacementForm.thumbnail) }}
    {{ form_widget(emplacementForm.thumbnail) }}
    {{ form_errors(emplacementForm.thumbnail) }}<hr>

    {{ form_label(emplacementForm.name) }}
    {{ form_widget(emplacementForm.name) }}
    {{ form_errors(emplacementForm.name) }}<hr>

    {{ form_label(emplacementForm.adress) }}
